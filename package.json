{
  "name": "gatsby-source-s3-image",
  "description": "GatsbyJS plugin to source images from S3-compliant APIs, with EXIF-extracting superpowers.",
  "version": "1.2.1",
  "author": "Jesse Stuart <hi@jessestuart.com>",
  "dependencies": {
    "aws-sdk": "2.441.0",
    "bluebird": "3.5.4",
    "exif-parser": "0.1.12",
    "lodash": "4.17.11",
    "luxon": "1.12.1",
    "mime-types": "2.1.24"
  },
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/preset-typescript": "7.3.3",
