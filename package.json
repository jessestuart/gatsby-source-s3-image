    "@semantic-release/git": "7.0.8",
    "@types/bluebird": "3.5.26",
    "@types/jest": "24.0.11",
    "@types/lodash": "4.14.123",
    "@types/luxon": "1.12.0",
    "@types/mime-types": "2.1.0",
